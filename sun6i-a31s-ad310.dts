/dts-v1/;
/include/ "sun6i-a31.dtsi"
/include/ "sunxi-common-regulators.dtsi"

/ {
	model = "zeroone AD310";
	compatible = "mele,m9", "allwinner,sun6i-a31";

	chosen {
		bootargs = "earlyprintk console=ttyS0,115200";
	};

	aliases {
		/* display0 = &hdmi; */
	};

	soc@01c00000 {
		mmc0: mmc@01c0f000 {
			pinctrl-names = "default";
			pinctrl-0 = <&mmc0_pins_a>, <&mmc0_cd_pin_m9>;
			vmmc-supply = <&reg_vcc3v3>;
			bus-width = <4>;
			cd-gpios = <&pio 7 22 0>; /* PH22 */
			cd-inverted;
			status = "okay";
		};

		/* FORESEE NCFSES78-08G */
		mmc2: mmc@01c11000 {
			pinctrl-names = "default";
			pinctrl-0 = <&mmc2_pins_a>;
			vmmc-supply = <&reg_vcc3v3>;
			bus-width = <4>;
			non-removable;
			status = "okay";
		};

		ahci: sata@01c18000 {
			compatible = "allwinner,sun4i-a10-ahci";
			reg = <0x01c18000 0x1000>;
			interrupts = <0 56 4>;
			clocks = <&pll6 0>, <&ahb1_gates 0>;
			target-supply = <&reg_ahci_5v>;
			status = "disabled";
		};

		pio: pinctrl@01c20800 {
			mmc0_cd_pin_m9: mmc0_cd_pin@0 {
				allwinner,pins = "PH22";
				allwinner,function = "gpio_in";
				allwinner,drive = <0>;
				allwinner,pull = <1>;
			};

			mmc2_pins_a: mmc2@0 {
				allwinner,pins = "PC6","PC7","PC8","PC9","PC10","PC11";
				allwinner,function = "mmc2";
				allwinner,drive = <2>;
				allwinner,pull = <0>;
			};

			led_pins_ad310: led_pins@0{
				allwinner,pins = "PH13";
				allwinner,function = "gpio_out";
				allwinner,drive = <0>;
				allwinner.pull = <0>;
			};
/*
            usb1_vbus_pin_ad310: usb1_vbus_pin@0{
                allwinner,pins = "PC27";
                allwinner,function = "gpio_out";
                allwinner,drive = <0>;
                allwinner,pull = <0>;
            }
*/						
		};

		/* REALTAK RTL8201CP */
		gmac: ethernet@01c30000 {
			pinctrl-names = "default";
			pinctrl-0 = <&gmac_pins_mii_a>;
			phy = <&phy1>;
			phy-mode = "mii";
			status = "okay";
			
			phy1: ethernet-phy@1{
				reg = <1>;
			};
		};

		uart0: serial@01c28000 {
			pinctrl-names = "default";
			pinctrl-0 = <&uart0_pins_a>;
			status = "okay";
		};

		usbphy: phy@01c19400 {
			usb1_vbus-supply = <&reg_usb1_vbus>;
			usb2_vbus-supply = <&reg_usb2_vbus>;
			status = "okay";
		};

		ehci0: usb@01c1a000 {
			status = "okay";
		};

		ohci0: usb@01c1a400 {
			status = "okay";
		};

		ehci1: usb@01c1b000 {
			status = "okay";
		};

		ohci1: usb@01c1b400 {
			status = "okay";
		};

		ohci2: usb@01c1c400 {
			status = "okay";
		};
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&led_pins_ad310>;

		blue {
			label = "ad310:blue:usr";
			gpios = <&pio 7 13 0>;
			default-state = "on";
		};
	};

	reg_usb1_vbus: usb1-vbus {
		status = "okay";
	};

	reg_usb2_vbus: usb2-vbus {
		status = "okay";
	};
};
